.devise
  = form_with(model:@posts, url: portfolio_index_path ,local: true) do |f|
    %h3
      投稿する
    = f.text_field :apptitle ,placeholder: "apptitle"
    %br/
    = f.text_field :image ,placeholder: "image"
    %br/
    = f.text_field :github ,placeholder: "github"
    %br/
    = f.text_area :text ,placeholder: "本文を入力"
    %br/
    = f.submit :登録
